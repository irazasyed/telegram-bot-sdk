  "type": "library",
  "homepage": "https://github.com/irazasyed/telegram-bot-sdk",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Syed Irfaq R.",
      "email": "syed+gh@lukonet.com",
      "homepage": "https://github.com/irazasyed"
    }
